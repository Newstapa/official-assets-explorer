<div class="col s12 m6 l4">
  <div id="official-<%- model.Person.uniqueId %>" class="card">
    <div class="card-content">
      <span class="card-title"><%- model.Person.name %></span>
      <span class="card-subtitle"><%- model.Position[0].Org3.title %><br><%- model.isElec ? model.Position[0].title : model.mainPos %></span>
    </div>
    <div class="card-action">
      <% model.Position.forEach(function(p) {%>
        <li><%- p.year %>ë…„ <%- p.Org3.title %> <%- p.title %></li>
      <% }) %>
      <span class="card-link-to-btn">
      <a class="btn btn-floating color-news">
        <img src="/img/arrow.png" alt="arrow-right" />
      </a>
      </span>
    </div>
  </div>
</div>
