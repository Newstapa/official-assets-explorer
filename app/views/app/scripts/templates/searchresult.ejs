<section id="search-result">
  <section class="container">
    <div class="row search-message">
      <div class="col s12">
        <h5 class="col s8 m12">총 <span class="value"><%- count %></span>개의 결과를 찾았습니다</h5>
        <button id="btn-scroll-searchbox" class="btn color-news col s4 hide-desktop" type="button" name="button">다시 검색하기</button>
      </div>
      <!-- <div class="col s3">
        <button id="btn-research" class="btn waves-effect color-news hide-desktop" type="button">다시 검색하기</button>
      </div> -->
    </div>
    <div class="row search-cards" style="position: relative;">

    </div>
    <section class="search-loading">
      <div class="preloader-wrapper small active">
        <div class="spinner-layer spinner-green-only">
          <div class="circle-clipper left">
            <div class="circle"></div>
          </div><div class="gap-patch">
            <div class="circle"></div>
          </div><div class="circle-clipper right">
            <div class="circle"></div>
          </div>
        </div>
      </div>
    </section>
  </section>
</section>
