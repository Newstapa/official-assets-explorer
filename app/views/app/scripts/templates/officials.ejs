<section class="container">
  <div class="row search-message">
    <div class="col s12">
      <h5>총 <%- count %>개의 결과를 찾았습니다.</h5>
    </div>
    <!-- <div class="col s3">
      <button id="btn-research" class="btn waves-effect color-news hide-desktop" type="button">다시 검색하기</button>
    </div> -->
  </div>
  <div class="row search-cards">
  <% officials.forEach(function(o) {%>
    <div class="col s12 m6 l4">
      <div id="official-<%- o.Person.uniqueId %>" class="card">
        <div class="card-content">
          <span class="card-title"><%- o.Person.name %></span>
          <span class="card-subtitle"><%= o.Position[o.Position.length - 1].title %></span>
        </div>
        <div class="card-action">
          <% o.Position.forEach(function(p) {%>
            <li><%- p.year %>년 <%- p.title %></li>
          <% }) %>
          <span class="card-link-to-btn">
          <a class="btn-floating btn-large waves-effect waves-light color-news"><i class="material-icons">trending_flat</i></a>
          </span>
        </div>
      </div>
    </div>
  <% }) %>
  </div>
</section>
